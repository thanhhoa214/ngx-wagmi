{
  "name": "ngwagmi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build example",
    "watch": "ng build --watch --configuration development",
    "wagmi:watch": "ng build ngx-wagmi --watch --configuration development",
    "wagmi:build": "ng build ngx-wagmi",
    "wagmi:publish": "cd projects/ngx-wagmi && npm version patch && cd ../../ && ng build ngx-wagmi && cd dist/ngx-wagmi && npm publish",
    "seedkit:watch": "ng build ngx-seedkit --watch --configuration development",
    "seedkit:build-tailwind": "tailwindcss -c projects/ngx-seedkit/tailwind.config.js -i ./projects/ngx-seedkit/src/styles/index.scss -o ./projects/ngx-seedkit/src/styles/index.css -m",
    "seedkit:build": "npm run seedkit:build-tailwind && ng build ngx-seedkit",
    "seedkit:publish": "cd projects/ngx-seedkit && npm version patch && cd ../../ && npm run seedkit:build && cd dist/ngx-seedkit && npm publish",
    "test": "ng test",
    "serve:ssr:ngx-wagmi-example": "node dist/ngx-wagmi-example/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/platform-server": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@angular/ssr": "^18.2.12",
    "@rainbow-me/rainbowkit": "^2.2.0",
    "@tanstack/angular-query-experimental": "^5.61.3",
    "@wagmi/connectors": "^5.5.0",
    "@wagmi/core": "^2.15.0",
    "express": "^4.18.2",
    "lucide-angular": "^0.462.0",
    "ngx-highlightjs": "^12.0.0",
    "ngx-seedkit": "^0.0.4",
    "ngx-wagmi": "^0.0.2",
    "ngxtension": "^4.1.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tailwind-merge": "^2.5.5",
    "tslib": "^2.3.0",
    "ua-parser-js": "^2.0.0",
    "viem": "^2.21.50"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@types/qrcode": "^1.5.5",
    "daisyui": "^4.12.14",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.4.2"
  }
}
